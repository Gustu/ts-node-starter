  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "prod": "tsc && npm run migrate && node build/src/server.js",
    "test": "jest --runInBand",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "migrate": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts migration:run",
    "migrate:test": "ts-node ./node_modules/typeorm/cli -f ./ormconfig-test.ts migration:run",
    "migrate:all": "npm run migrate && npm run migrate:test",
    "migration:generate": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts migration:generate"
  },
  "dependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.8",
    "@types/superagent": "^4.1.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-rate-limit": "^3.5.0",
    "express-validator": "^5.3.1",
    "forever": "^1.0.0",
    "morgan": "^1.9.1",
    "pg": "^7.10.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^7.3.2",
    "superagent": "^5.0.3",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "typeorm": "^0.2.16",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "jest": "^24.7.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.32"
  }
}
